{
  "name": "nyc-factfinder-api",
  "version": "0.0.1",
  "scripts": {
    "start": "node ./bin/www",
    "devstart": "NODE_ENV=development nodemon ./bin/www",
    "saveoutput": "if [ \"$NODE_ENV\" = \"development\" ]; then curl -f http://localhost:4000/profile/1/demographic?compare=0 | node_modules/.bin/prettyjson > test-output/1-demographic-0.yaml; fi",
    "lint": "eslint -c .eslintrc lib",
    "test": "NODE_ENV=test nodemon --ext js --watch . --exec 'mocha ./test' --delay 1 || true"
  },
  "pre-commit": [
    "saveoutput",
    "test"
  ],
  "license": "ISC",
  "dependencies": {
    "apicache": "^1.1.0",
    "body-parser": "^1.15.2",
    "compression": "1.7.1",
    "cwait": "1.1.1",
    "d3": "^4.13.0",
    "d3-collection": "^1.0.4",
    "debug": "^3.1.0",
    "dotenv": "^4.0.0",
    "ember-metal": "^0.9.2",
    "ember-source": "^2.18.0",
    "express": "^4.16.1",
    "hot-formula-parser": "^2.3.1",
    "lodash": "^4.17.5",
    "mongoose": "^4.13.4",
    "mongoose-auto-increment": "^5.0.1",
    "numeral": "^2.0.6",
    "pg": "^7.4.1",
    "pg-error": "^1.0.0",
    "request": "^2.83.0",
    "request-promise": "^4.2.2",
    "sha1": "^1.1.1",
    "slug": "^0.9.1",
    "underscore": "^1.8.3"
  },
  "engines": {
    "node": "8.6.0",
    "npm": "5.3.0"
  },
  "devDependencies": {
    "babel-eslint": "^8.0.1",
    "chai": "4.1.2",
    "chai-http": "3.0.0",
    "eslint": "^4.8.0",
    "eslint-config-airbnb-base": "^12.0.2",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-import": "^2.6.1",
    "expect": "22.4.0",
    "mocha": "5.0.2",
    "prettyjson": "1.2.1",
    "should": "13.2.1",
    "supertest": "3.0.0"
  }
}
